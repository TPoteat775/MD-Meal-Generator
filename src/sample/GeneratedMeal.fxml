<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.GeneratedMealController">
   <children>
      <AnchorPane prefHeight="80.0" prefWidth="600.0" style="-fx-background-color: #7b241c;">
         <children>
            <Text fill="#ffc300" layoutX="210.0" layoutY="49.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Generated Meals">
               <font>
                  <Font size="24.0" />
               </font>
            </Text>
         </children>
      </AnchorPane>
      <AnchorPane layoutY="80.0" prefHeight="321.0" prefWidth="600.0" style="-fx-background-color: #FFC300;">
         <children>
            <ImageView fx:id="mealImage" fitHeight="200.0" fitWidth="200.0" layoutX="14.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true" />
            <ListView fx:id="mealInfo" layoutX="247.0" layoutY="14.0" prefHeight="200.0" prefWidth="345.0" style="-fx-background-color: #FFC300;" />
            <Button fx:id="backToMain" layoutX="165.0" layoutY="282.0" mnemonicParsing="false" onAction="#backToMainOnAction" style="-fx-background-color: #7b241c;" text="Back to Main Menu" textFill="#ffc300" />
            <Button fx:id="backToGen" layoutX="300.0" layoutY="282.0" mnemonicParsing="false" onAction="#backToGenOnAction" style="-fx-background-color: #7b241c;" text="Generate More Meals" textFill="#ffc300" />
            <Button fx:id="viewNext" layoutX="300.0" layoutY="237.0" mnemonicParsing="false" onAction="#viewNextOnAction" style="-fx-background-color: #7b241c;" text="View Next" textFill="#ffc300" />
            <CheckBox fx:id="favorite" layoutX="522.0" layoutY="232.0" mnemonicParsing="false" onAction="#favoriteOnAction" text="Favorite" />
            <CheckBox fx:id="dislike" alignment="TOP_LEFT" layoutX="14.0" layoutY="232.0" mnemonicParsing="false" onAction="#dislikeOnAction" text="Dislike" />
            <Button fx:id="viewPrevious" layoutX="194.0" layoutY="237.0" mnemonicParsing="false" onAction="#viewPrevOnAction" style="-fx-background-color: #7b241c;" text="View Previous" textFill="#ffc300" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
